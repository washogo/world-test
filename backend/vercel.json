{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "dist/src/route/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/route/server.js"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/dist/src/route/server.js" }]
}
